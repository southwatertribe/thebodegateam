{
  "name": "react-dynamic-star",
  "version": "0.0.2",
  "description": "Single component to generate dynamic star svg",
  "main": "./dist/rds.umd.js",
  "module": "./dist/rds.es.js",
  "types": "./dist/rds.d.ts",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "jest",
    "lint": "eslint src lib --ext ts,tsx",
    "lint:fix": "yarn lint --fix",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/rds.es.js",
      "require": "./dist/rds.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igorwessel/react-dynamic-star.git"
  },
  "keywords": [
    "react dynamic star",
    "react star rating"
  ],
  "author": "Igor Wessel",
  "bugs": {
    "url": "https://github.com/igorwessel/react-dynamic-star/issues"
  },
  "homepage": "https://github.com/igorwessel/react-dynamic-star",
  "peerDependencies": {
    "react": ">=17.x"
  },
  "resolutions": {
    "@types/react": "17.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.2",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.1",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "@vitejs/plugin-react": "1.0.0",
    "babel-eslint": "10.1.0",
    "eslint": "7.32.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-config-standard": "16.0.3",
    "eslint-config-standard-jsx": "10.0.0",
    "eslint-config-standard-react": "11.0.1",
    "eslint-plugin-flowtype": "6.1.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jest": "25.2.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "4.12.4",
    "jest": "27.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "ts-jest": "27.0.7",
    "typescript": "4.4.4",
    "vite": "2.6.4",
    "vite-plugin-dts": "0.9.2",
    "vite-plugin-libcss": "1.0.5",
    "vite-plugin-linter": "1.0.1",
    "vite-tsconfig-paths": "3.3.17"
  }
}
